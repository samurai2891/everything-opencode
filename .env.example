# =============================================================================
# Opencode Environment Variables
# =============================================================================
#
# Copy this file to .env and fill in your values.
# These variables are used by opencode.json and batch launch scripts.
#
# =============================================================================

# -----------------------------------------------------------------------------
# Model Configuration
# -----------------------------------------------------------------------------
# Default model for all agents and commands.
# Can be overridden per-session via environment variables.
#
# Available models:
#   OpenAI:    openai/gpt-5.2-codex, openai/gpt-4o, openai/o1, openai/o3
#   Anthropic: anthropic/claude-sonnet-4-20250514, anthropic/claude-opus-4-20250514
#   Google:    google/gemini-2.5-pro, google/gemini-2.5-flash
#   Z.AI:      z-ai/glm-4.7, z-ai/glm-4.7-flash
#   DeepSeek:  deepseek/deepseek-chat, deepseek/deepseek-coder
#   Mistral:   mistral/mistral-large-latest, mistral/codestral-latest

OPENCODE_MODEL=openai/gpt-5.2-codex
OPENCODE_SMALL_MODEL=openai/gpt-5.2-codex
OPENCODE_PLAN_MODEL=openai/gpt-5.2-codex

# -----------------------------------------------------------------------------
# API Keys (for non-Opencode subscription providers)
# -----------------------------------------------------------------------------
# If using Opencode subscription, these may not be required.
# Only set if using direct API access.

# OpenAI API Key (for Codex 5.2, GPT-4o, etc.)
OPENAI_API_KEY=

# Anthropic (Claude)
ANTHROPIC_API_KEY=

# Google (Gemini)
GOOGLE_API_KEY=

# Z.AI (GLM)
ZAI_API_KEY=
ZAI_CODING_API_KEY=

# DeepSeek
DEEPSEEK_API_KEY=

# Mistral
MISTRAL_API_KEY=

# -----------------------------------------------------------------------------
# MCP Server Configuration
# -----------------------------------------------------------------------------

# GitHub MCP Server
GITHUB_PAT=

# Firecrawl MCP Server (optional)
FIRECRAWL_API_KEY=

# Supabase MCP Server (optional)
SUPABASE_PROJECT_REF=

# -----------------------------------------------------------------------------
# Batch Launch Configuration
# -----------------------------------------------------------------------------
# Used by scripts/batch-launch.sh and scripts/sdk_batch_launcher.py

# Number of parallel workers for batch operations
OPENCODE_WORKERS=4

# Default tmux session name
OPENCODE_SESSION=opencode-batch
